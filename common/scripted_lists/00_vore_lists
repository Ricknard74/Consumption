prey_list = {
    every_relation = {
        type = prey
    }
}

predator_list = {
    every_character = {
        limit = {
            has_relation_predator
        }
    }
}

predator_prey_list = {
    every_character = {
        limit = {
            OR = {
                has_relation_prey = yes
                has_relation_predator = yes
            }
        }
    }
}

